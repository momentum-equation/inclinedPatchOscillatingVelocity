FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      transportProperties;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

transportModel Newtonian;

// Laminar viscosity
nu              [0 2 -1 0 0 0 0] 3.813e-05;

// Laminar Prandtl number
Pr              [0 0 0 0 0 0 0] 0.71;

// Turbulent Prandtl number
Prt             [0 0 0 0 0 0 0] 0.85;

